# Firebase Web configNEXT_PUBLIC_FIREBASE_API_KEY=

NEXT_PUBLIC_FIREBASE_API_KEY=YOUR_API_KEYNEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=

NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=YOUR_PROJECT.firebaseapp.comNEXT_PUBLIC_FIREBASE_PROJECT_ID=

NEXT_PUBLIC_FIREBASE_PROJECT_ID=YOUR_PROJECT_IDNEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=

NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=YOUR_PROJECT_ID.firebasestorage.appNEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=

NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=YOUR_SENDER_IDNEXT_PUBLIC_FIREBASE_APP_ID=

NEXT_PUBLIC_FIREBASE_APP_ID=YOUR_APP_ID

# Cloud Functions region (defaults to us-central1 if not set)
# NEXT_PUBLIC_FIREBASE_FUNCTIONS_REGION=us-central1

# App Check (optional but recommended when Functions enforcement is enabled)
# Enable App Check in Firebase Console for your Web app and copy the reCAPTCHA v3 site key
# Then uncomment and fill the values below:
# NEXT_PUBLIC_ENABLE_APPCHECK=true
# NEXT_PUBLIC_RECAPTCHA_V3_SITE_KEY=YOUR_RECAPTCHA_V3_SITE_KEY
# For local dev, you can use a debug token so you don't have to solve reCAPTCHA each time:
# Use 'auto' to let the SDK generate a token which you can copy from the console logs in App Check debug view
# NEXT_PUBLIC_APPCHECK_DEBUG_TOKEN=auto

# Use emulators locally (optional)
# NEXT_PUBLIC_USE_FIREBASE_EMULATORS=true
